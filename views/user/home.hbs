    <style>
        .nameBoard {
            height: 50%;
            margin-top: 2%;
            margin: auto;
            margin-left: 10px;
            border: none;
            background-color: aqua;
        }

        .form-container {
            width: 100%;
            margin: auto;
        }

        .form-container input {
            width: 100%;
            border: none;
        }

        .btn-submit {
            background-color: rgb(98, 131, 214);
            border: 1px solid green;
            margin: auto;
        }

        .btns {
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100%;

        }

        .btn {
            height: 30px;
            gap: 5px;
            margin-top: 36px;
            border-radius: 20px;
            padding: 3px;
        }

        .searchBox {
            border: none;
            width: 350px;
            height: 39px;
            border-top-left-radius: 30px;
            border-bottom-left-radius: 30px;

            border: 3px solid rgb(212, 237, 208);
            text-align: center;
        }

        .searchBox:hover {
            border: 2px solid rgb(25, 236, 67));
        }

        .searchBtn {
            background-color: white;
            border: 1px solid greenyellow;
            border: none;
            height: 40px;
            border-top-right-radius: 30px;
            border-bottom-right-radius: 30px;
        }

        .searchBtn:hover {
            box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;
        }

        .submitBtn {
            background-color: white;
            border: 1px solid greenyellow;
            border: none;
            height: 39px;
            width: 70px;
            border-top-right-radius: 30px;
            border-bottom-right-radius: 30px;
        }
    </style>
</head>

